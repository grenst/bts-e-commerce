    "@commercetools/platform-sdk": "^8.12.1",
    "@commercetools/sdk-client-v2": "^3.0.0",
    "@commercetools/sdk-middleware-auth": "^8.0.1",
    "@commercetools/sdk-middleware-http": "^8.0.1",
    "@eslint/plugin-kit": "^0.3.4",
    "axios": "^1.10.0",
    "eslint-plugin-unicorn": "^60.0.0",
    "gh-pages": "^6.3.0",
    "gsap": "^3.13.0",
    "postal-code-validator": "^1.0.3",
    "postcode-validator": "^3.10.5",
    "zod": "^3.25.76",
    "zustand": "^5.0.6"
