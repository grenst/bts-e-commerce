{
  "name": "bts-e-commerce",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky",
    "test": "jest",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,html,json,md}\"",
    "lint-fix": "eslint . --fix"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.5",
    "@types/jest": "^29.5.14",
    "@types/sass": "^1.43.1",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "autoprefixer": "^10.4.21",
    "buffer": "^6.0.3",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.5.1",
    "postcss": "^8.5.3",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.5.3",
    "sass": "^1.87.0",
    "tailwindcss": "^4.1.5",
    "ts-jest": "^29.3.2",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.31.1",
    "vite": "^6.3.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@commercetools/platform-sdk": "^8.8.0",
    "@commercetools/sdk-client-v2": "^3.0.0",
    "@commercetools/sdk-middleware-auth": "^8.0.1",
    "@commercetools/sdk-middleware-http": "^8.0.1",
    "gh-pages": "^6.3.0",
    "zod": "^3.24.4"
  }
}
